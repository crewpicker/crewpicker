# Ignore bundler config.
/.bundle
/vendor/bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore secrets
/config/master.key
/config/database.yml
/config/config.yml

# Ignore uploads
/public/system
/public/assets

# bfg - password removal tool
passwords.txt
..bfg-report

# Ignore yarn stuff
/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity
/.npmrc

# Ignore compiled assets
/app/assets/builds/*
!/app/assets/builds/.keep

.direnv
