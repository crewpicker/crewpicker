<h1>Listing access_levels</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @access_levels.each do |access_level| %>
  <tr>
    <td><%= access_level.name %></td>
    <td><%= link_to 'Show', access_level %></td>
    <td><%= link_to 'Edit', edit_access_level_path(access_level) %></td>
    <td><%= link_to 'Destroy', access_level, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Access level', new_access_level_path %>
