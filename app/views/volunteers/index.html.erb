<h1>Listing volunteers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Birthday</th>
    <th>Email</th>
    <th>Phone</th>
    <th>Group</th>
    <th>Access level</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @volunteers.each do |volunteer| %>
  <tr>
    <td><%= volunteer.name %></td>
    <td><%= volunteer.address %></td>
    <td><%= volunteer.birthday %></td>
    <td><%= volunteer.email %></td>
    <td><%= volunteer.phone %></td>
    <td><%= volunteer.group_id %></td>
    <td><%= volunteer.access_level %></td>
    <td><%= link_to 'Show', volunteer %></td>
    <td><%= link_to 'Edit', edit_volunteer_path(volunteer) %></td>
    <td><%= link_to 'Destroy', volunteer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Volunteer', new_volunteer_path %>
